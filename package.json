{
  "name": "clean-tagged-string",
  "version": "0.0.6",
  "description": "A simple utility function to clean ES6 template strings.",
  "main": "index.js",
  "scripts": {
    "build:2015": "babel src/index.js -o ./2015/index.js --presets=stage-3,es2015",
    "build:2017": "babel src/index.js -o ./index.js --presets=stage-3,es2017",
    "build": "npm run build:2015; npm run build:2017",
    "test": "ava --tap",
    "dev": "concurrently \"ava --fail-fast --watch\" \"babel src/index.js -o ./index.js --watch\"",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxigy/clean-tagged-string.git"
  },
  "keywords": [
    "es6",
    "es2015",
    "template",
    "strings",
    "tagged",
    "template",
    "strings",
    "node",
    "javascript"
  ],
  "author": "Rishat Muhametshin <rishat@rishat.us> (http://rishat.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taxigy/clean-tagged-string/issues"
  },
  "homepage": "https://github.com/taxigy/clean-tagged-string#readme",
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.4.0"
  }
}
